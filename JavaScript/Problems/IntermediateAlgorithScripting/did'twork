function whatIsInAName(collection, source) {
  let src = Object.keys(source);
  let x = 0;
  let newarr = [];
  while (src.length > x) {
    for (let i = 0; i < collection.length; i++) {
      if (collection[i].hasOwnProperty(src[x])) {
      } else {
        collection.splice(i, 1);
      }
    }
    x++;
  }
  return collection;
}

function whatIsInAName(collection, source) {
  let obj;
  if (collection.length == 0) {
    objeto = collection[-1];
  }
  //We need to use Object.key or Object.hasOwnPropertyNames to take an array of props;
  let prop = Object.keys(source);
  // Use console to make sure
  console.log(prop);
  let ownProps = [];
  //Then we need to loop to our props, to see if any of them is a prop of out collections;
  for (let i = -1; i < prop.length; i++) {
    if (objeto.hasOwnProperty(prop)) {
      ownProps.push({ [prop]: prop });
    }
  }
  return ownProps;
}
